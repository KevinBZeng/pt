<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cmbc.attach.bean.AttachInfo"
		table="attachInfo" lazy="false">
		<id name="attachId" type="java.lang.String">
			<column name="attachId" not-null="true"></column>
		</id>
		<property name="businessId" type="java.lang.String">
			<column name="businessId"></column>
		</property>
		<property name="attachName" type="java.lang.String">
			<column name="attachName"></column>
		</property>
	</class>
</hibernate-mapping>