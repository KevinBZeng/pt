<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cmbc.funcmanage.bean.RedempBook" table="redempBook" lazy="false">
		<id name="contractId" type="java.lang.String">
			<column name="contractId" not-null="true"></column>
		</id>	
		<property name="productId" type="java.lang.String">
			<column name="productId"></column>
		</property>
		<property name="productCode" type="java.lang.String">
			<column name="productCode"></column>
		</property>
		<property name="productName" type="java.lang.String">
			<column name="productName"></column>
		</property>
		<property name="benefitDate" type="java.lang.String">
			<column name="benefitDate"></column>
		</property>
		<property name="dueDate" type="java.lang.String">
			<column name="dueDate"></column>
		</property>
		<property name="plannedBenefit" type="java.lang.String">
			<column name="plannedBenefit"></column>
		</property>
		<property name="customeId" type="java.lang.String">
			<column name="customeId" not-null="true"></column>
		</property>
		<property name="customeName" type="java.lang.String">
			<column name="customeName"></column>
		</property>
		<property name="signAccount" type="java.lang.String">
			<column name="signAccount"></column>
		</property>
		<property name="money" type="java.lang.String">
			<column name="money"></column>
		</property>
		<property name="belongDepartment" type="java.lang.String">
			<column name="belongDepartment"></column>
		</property>
		<property name="signDepartment" type="java.lang.String">
			<column name="signDepartment"></column>
		</property>
		<property name="redempStartDate" type="java.lang.String">
			<column name="redempStartDate"></column>
		</property>
		<property name="redempConformDate" type="java.lang.String">
			<column name="redempConformDate"></column>
		</property>
		<property name="openDay" type="java.lang.String">
			<column name="openDay"></column>
		</property>
		<property name="redeemBegin" type="java.lang.String">
			<column name="redeemBegin"></column>
		</property>
		<property name="redeemEnd" type="java.lang.String">
			<column name="redeemEnd"></column>
		</property>
		<property name="rollBenefit" type="java.lang.String">
			<column name="rollBenefit"></column>
		</property>
		<property name="redempStatus" type="java.lang.String">
			<column name="redempStatus"></column>
		</property>
	</class>
</hibernate-mapping>