<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.cmbc.sa.bean.Permission" table="permission" schema="dbo" catalog="rsosdb_pbank">
        <id name="permissionId" type="java.lang.String">
            <column name="permissionId" />
            <generator class="assigned" />
        </id>
        <property name="permissionName" type="java.lang.String">
            <column name="permissionName" />
        </property>
        <property name="url" type="java.lang.String">
            <column name="url" />
        </property>
        <property name="imageUrl" type="java.lang.String">
            <column name="imageUrl" />
        </property>
        <property name="checked" type="java.lang.Integer">
            <column name="checked" />
        </property>
        <property name="parentId" type="java.lang.String">
            <column name="parentId" />
        </property>
        <property name="type" type="java.lang.Integer">
            <column name="type" />
        </property>
    </class>
</hibernate-mapping>
