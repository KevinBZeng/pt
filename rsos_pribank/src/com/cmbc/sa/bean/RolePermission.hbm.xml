<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.cmbc.sa.bean.RolePermission" table="rolePermission" schema="dbo" catalog="rsosdb_pbank">        
		<id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="business" type="java.lang.Integer">
            <column name="business" />
        </property>
        <property name="roleId" type="java.lang.String">
            <column name="roleId" />
        </property>
        <property name="permissionId" type="java.lang.String">
            <column name="permissionId" />
        </property>
    </class>
</hibernate-mapping>
